        "namespace": "nglp",
        "identifier": "unit"
      },
      "select": {
        "direct": "descendants"
      },
      "order": [
        { "path": "entity.title", "direction": "asc" }
      ],
      "render": {
        "mode": "tree"
      },
      "position": 2
    },
    {
      "id": "etd",
      "name": "Electronic Theses and Dissertations",
      "filter": {
        "schemas": [
          {
            "namespace": "nglp",
            "identifier": "dissertation"
          }
        ]
      },
      "select": {
        "direct": "descendants",
        "links": {
          "contains": true
        }
      },
      "order": [
        { "path": "entity.published", "direction": "desc", "nulls": "last" },
        { "path": "entity.title", "direction": "asc" }
      ],
      "position": 3
    },
    {
      "id": "papers",
      "name": "Papers",
      "filter": {
        "schemas": [
          {
            "namespace": "nglp",
            "identifier": "paper"
          }
        ]
      },
      "select": {
        "direct": "descendants",
        "links": {
          "contains": true
        }
      },
      "order": [
        { "path": "entity.published", "direction": "desc", "nulls": "last" },
        { "path": "entity.title", "direction": "asc" }
      ],
      "position": 4
    },
    {
      "id": "journals",
      "name": "Journals",
      "filter": {
        "schemas": [
          {
            "namespace": "nglp",
            "identifier": "journal"
          }
        ]
      },
      "select": {
        "direct": "descendants"
      },
      "order": [
        { "path": "entity.title", "direction": "asc" }
      ],
      "position": 5
    },
    {
      "id": "issues",
      "name": "Journal Issues",
      "filter": {
        "schemas": [
          {
            "namespace": "nglp",
            "identifier": "journal_issue"
          }
        ]
      },
      "select": {
        "direct": "descendants"
      },
      "order": [
        { "path": "entity.published", "direction": "desc", "nulls": "last" },
        { "path": "entity.created_at", "direction": "desc", "nulls": "last" }
