{
  "namespace": "default",
  "identifier": "community",
  "name": "Default Community",
  "version": "1.0.0",
  "kind": "community",
  "orderings": [
    {
      "id": "series",
      "name": "Series",
      "filter": {
        "schemas": [
          {
            "namespace": "nglp",
            "identifier": "series"
          }
        ]
      },
      "select": {
        "direct": "descendants"
      },
      "order": [
        { "path": "entity.title", "direction": "asc" }
      ],
      "position": 1
    },
    {
      "id": "units",
      "name": "Units",
      "filter": {
        "schemas": [
          {
            "namespace": "nglp",
            "identifier": "unit"
          }
        ]
      },
