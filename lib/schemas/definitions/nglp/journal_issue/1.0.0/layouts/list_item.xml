<?xml version="1.0" encoding="UTF-8"?>
<list-item>
  <templates>
    <list-item>
      <slots>
        <header>
          {{entity.title}}
        </header>
        <subheader>
          {{entity.subtitle}}
        </subheader>
        <description/>
        <meta-a/>
        <meta-b/>
        <context-a>
          {{ancestors.journal.title}}
        </context-a>
        <context-b>
          {% if ancestors.volume %} {{ancestors.volume.title}} {% endif %}
        </context-b>
        <context-c/>
      </slots>
    </list-item>
  </templates>
</list-item>
