<?xml version="1.0" encoding="UTF-8"?>
<config layout="supplementary" has-background="true" has-variant="false">
  <description>
    A template for describing how an entity's supplementary pages
    (contributors, metrics) should render.

    Contained within the `SUPPLEMENTARY` layout.
  </description>
  <prop name="background" kind="background">
    <default>
      <![CDATA[
        "none"
      ]]>
    </default>
  </prop>
  <slot name="contributors_label" kind="inline">
    <default-template>
      <![CDATA[
        Contributors
      ]]>
    </default-template>
  </slot>
  <slot name="metrics_label" kind="inline">
    <default-template>
      <![CDATA[
        Metrics
      ]]>
    </default-template>
  </slot>
</config>
